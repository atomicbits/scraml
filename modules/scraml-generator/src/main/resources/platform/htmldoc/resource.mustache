            <ul class="nav nav-tabs">

                <!--method request tab-->
                <li class="active">
                    <a href="#{{uniqueId}}_{{actionType.action}}_request" data-toggle="tab">Request</a>
                </li>

                <!--method response tab-->
                <li>
